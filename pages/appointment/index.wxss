/* pages/appointment/index.wxss */

radio .wx-radio-input.wx-radio-input-checked {
  background: #437eff !important;
  border: 1px solid #437eff !important;
}

/* 提示 */

.prompt {
  display: flex;
  align-items: center;
  padding: 0rpx 30rpx;
  height: 50rpx;
  font-size: 12px;
}

.prompt_info {
  background-color: #e1ebff;
  color: #2c6fff;
}

.prompt_error {
  background-color: #ffe9e9;
  color: #ff5353;
}

.from,.from2 {
  padding: 0 40rpx;
  background: #fff;
}

.mb20rpx {
  margin-bottom: 20rpx;
}

.from>view {
  display: flex;
  align-items: center;
  height: 120rpx;
}

.from>view>text {
  float: left;
  width: 200rpx;
}
.photo{
  position: relative;
}
.photo>text{
  position: absolute;
  right: 10rpx;
  width: auto !important;
}
.from>view>input, .from>view>picker, .from>view>radio-group, .from>view>view {
  float: right;
  width: calc(100% - 210rpx);
}

.from>view>radio-group label {
  display: inline-block;
  width: 50%;
}

.from>view {
  border-bottom: 1px solid #f9f9f9;
}

.from>view:last-of-type {
  border: none;
}
.id_photo{
  padding: 40rpx;
}
.id_photo .title {
  margin: 0 0 40rpx;
}
.content{
  text-align: center;
  /* display: flex; */
  /* justify-content: center; */
}
.content>.txt{
  margin: 30rpx 0;
  text-align: center;
  color:#9b9b9b;
}
.id_1,.id_2{
  display: block;
  margin: 0 auto;
  padding: 30rpx;
  width: 450rpx;
  height: 300rpx;
  background-size: 100% 100%;
}
.id_1{
  background-image: url('data:image/png;base64,')
}
.id_2{
  background-image: url('data:image/png;base64,')
}
.id_1>view,.id_2>view{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}
.id_1>view>image,.id_2>view>image{
  position: absolute;
  width: 100%;
  height: 100%;
}
